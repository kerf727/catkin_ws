        <visual name="">
            <origin xyz="0.0 0.0 0.0" rpy="0.0 0.0 0.0"/>
            <geometry>
                <mesh filename="package://hexapod_description/meshes/body.dae"/>
            </geometry>
        </visual>
        <collision>
            <origin xyz="0.0 0.0 0.0" rpy="0.0 0.0 0.0"/>
            <geometry>
                <mesh filename="package://hexapod_description/meshes/body.dae"/>
            </geometry>
        </collision>
    </link>

    <!-- ======================= Gazebo collision ======================= -->

    <gazebo reference="tibia_L1">
        <kp>1000000.0</kp>        <!--contact stiffness-->
        <kd>100.0</kd>        <!--damping-->
        <collision name="tibia_L1_collision">
            <surface>
                <friction>
                    <ode>
                        <mu>1.0</mu>
                        <mu2>1.0</mu2>
                    </ode>
                </friction>
            </surface>
        </collision>

