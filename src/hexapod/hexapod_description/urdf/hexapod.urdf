    <!-- ========================== Feet ========================== -->

    <link name="foot_L1"/>
    <link name="foot_R1"/>
    <link name="foot_L2"/>
    <link name="foot_R2"/>
    <link name="foot_L3"/>
    <link name="foot_R3"/>

    <!-- ========================== Hip Joints ========================== -->

    <joint name="joint_hip_L1" type="revolute">
        <origin xyz="-0.1016 0.1524 0" rpy="1.57 0 -1.57"/>
        <limit lower="-3" upper="3" effort="30.0" velocity="6"/>
        <parent link="body"/>
        <child link="femur_L1"/>
        <axis xyz="0 1 1"/>
        <safety_controller k_position="100.0" k_velocity="40.0" soft_lower_limit="-3" soft_upper_limit="3"/>
    </joint>
    <joint name="joint_hip_R1" type="revolute">
        <origin xyz="-0.1016 0.1524 0" rpy="1.57 0 -1.57"/>
        <limit lower="-3" upper="3" effort="30.0" velocity="6"/>
        <parent link="body"/>
        <child link="femur_R1"/>
        <axis xyz="0 1 1"/>
        <safety_controller k_position="100.0" k_velocity="40.0" soft_lower_limit="-3" soft_upper_limit="3"/>
    </joint>
    <joint name="joint_hip_L2" type="revolute">
