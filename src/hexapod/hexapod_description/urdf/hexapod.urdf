    <!-- ========================== Foot Joints ========================== -->

    <joint name="joint_foot_L1" type="fixed">
        <origin xyz="0.2032 0 0" rpy="0 0 0 "/>
        <parent link="tibia_L1"/>
        <child link="foot_L1"/>
    </joint>
    <joint name="joint_foot_R1" type="fixed">
